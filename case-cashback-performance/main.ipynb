   "source": [
    "data[\"lucro\"] = data[\"comissão\"] - data[\"cashback\"]\n",
    "data[\"take rate\"] = ((data[\"comissão\"] / data[\"vendas totais\"])).round(3) * 100\n",
    "data[\"cashback rate\"] = ((data[\"cashback\"] / data[\"vendas totais\"])).round(3) * 100\n",
    "data[\"cashback/comissão\"] = ((data[\"cashback\"] / data[\"comissão\"])).round(3) * 100\n",
    "data"
   ]
