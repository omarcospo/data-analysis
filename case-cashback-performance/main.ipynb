   "source": [
    "**Correções na base**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "data = pd.read_csv(\"Base de dados.csv\")\n",
    "\n",
    "data[\"Data\"] = pd.to_datetime(data[\"Data\"], format=\"%m-%d-%y\")\n",
    "\n",
    "for column in [\"comissão\", \"cashback\", \"vendas totais\"]:\n",
    "    data[column] = (\n",
    "        data[column]\n",
    "        .str.replace(\"$\", \"\", regex=False)\n",
    "        .str.replace(\".\", \"\", regex=False)\n",
    "        .str.replace(\",\", \".\", regex=False)\n",
    "        .astype(float)\n",
    "    )"
   ]
  },
