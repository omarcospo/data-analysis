   "source": [
    "grupos = (\n",
    "    data.groupby(\"Grupos de usuários\")\n",
    "    .agg(\n",
    "        {\n",
    "            \"compradores\": \"sum\",\n",
    "            \"vendas totais\": \"sum\",\n",
    "            \"comissão\": \"sum\",\n",
    "            \"cashback\": \"sum\",\n",
    "            \"lucro\": \"sum\",\n",
    "            \"take rate\": \"mean\",\n",
    "            \"cashback rate\": \"mean\",\n",
    "            \"cashback/comissão\": \"mean\",\n",
    "        }\n",
    "    )\n",
    "    .reset_index()\n",
    ")\n",
    "\n",
    "grupos[\"take rate\"] = grupos[\"take rate\"].round(1)\n",
    "grupos[\"cashback rate\"] = grupos[\"cashback rate\"].round(1)\n",
    "grupos[\"cashback/comissão\"] = grupos[\"cashback/comissão\"].round(1)\n",
    "\n",
    "grupos = grupos.sort_values(by=\"lucro\", ascending=False)\n",
    "grupos"
   ]
